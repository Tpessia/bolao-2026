{
    "swagger": "2.0",
    "info": {
        "title": "Bolao 2026",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/health": {
            "get": {
                "description": "Get server status",
                "produces": [
                    "text/plain"
                ],
                "summary": "Health Check",
                "responses": {
                    "200": {
                        "description": "Healthy! 2024-01-01 12:00:00",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/health-detailed": {
            "post": {
                "description": "Get detailed server status",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "Health Check Detailed",
                "parameters": [
                    {
                        "description": "User Data",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/config.HealthDetailedRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/config.HealthDetailedResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "config.HealthDetailedRequest": {
            "type": "object",
            "required": [
                "input"
            ],
            "properties": {
                "input": {
                    "type": "string",
                    "minLength": 3
                }
            }
        },
        "config.HealthDetailedResponse": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                }
            }
        }
    }
}